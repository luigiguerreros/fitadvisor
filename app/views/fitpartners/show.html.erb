<div class="page-header">
  <%= link_to fitpartners_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_fitpartner_path(@fitpartner), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show fitpartner</h1>
</div>

<div class="row">
  <div class="col-md-6">
    <dl class="dl-horizontal">

  <dt>Especialidad:</dt>
  <dd><%= @fitpartner.especialidad %></dd>

  <dt>Costo:</dt>
  <dd><%= @fitpartner.costo %></dd>

  <dt>Tipo costo:</dt>
  <dd><%= @fitpartner.tipo_costo %></dd>

  <dt>Disponibilidad:</dt>
  <dd><%= @fitpartner.disponibilidad %></dd>

  <dt>Descripcion:</dt>
  <dd><%= @fitpartner.descripcion %></dd>

  <dt>Address:</dt>
  <dd><%= @fitpartner.address %></dd>

  <dt>Latitude:</dt>
  <dd><%= @fitpartner.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @fitpartner.longitude %></dd>
</dl>
  </div>
  <div class="col-md-6">
    <%= image_tag "http://maps.google.com/maps/api/staticmap?size=450x300&sensor=false&zoom=16&markers=#{@fitpartner.latitude}%2C#{@fitpartner.longitude}" %>
  </div>
</div>